<%- await include('partials/header.ejs') -%>
<meta name="description" content="<%= summary %>">
<% if (jsonLD) { -%>
<script type="application/ld+json"><%- jsonLD %></script>
<% } -%>
<link rel="alternate" type="application/rss+xml" title="RSS" href="<%= rssUrl %>">
<link rel="canonical" href="https://www.hebcal.com/shabbat?<%= geoUrlArgs %>">
<link rel="stylesheet" type="text/css" href="/i/typeahead1.css">
<style>
ul.hebcal-results {
  list-style-type:none;
  padding-left: 0;
}
ul.hebcal-results li {
  margin-bottom: 11px;
  font-size: 1.15rem;
  font-weight: 300;
  line-height: normal;
}
</style>
</head>
<body>
<%- await include('partials/navbar.ejs') -%>
<div class="container">
<div id="content">
<div class="row mt-3">
<div class="col-md-8">
<h2><%= Shabbat %> Times for <%= location.getShortName() %></h2>
<h5 class="text-muted"><%= location.getName() %></h5>
<% if (typeof message !== 'undefined') { -%>
<div class="alert alert-danger alert-dismissible" role="alert">
  <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
  <%= message %>
</div><!-- .alert -->
<% } -%>
<%- await include('partials/location-warnings.ejs') -%>
<div class="float-start">
  <svg width="60" height="60" class="me-1 mb-1"><use xlink:href="<%=cspriteHref%>#candles"></use></svg>
</div>
<%- await include('partials/shabbat-items.ejs') -%>
<div class="mt-4">
<a class="btn btn-outline-primary btn-sm mb-2 me-1" title="Compact candle-lighting times for <%= hyear %>" href="/shabbat/fridge.cgi?<%= geoUrlArgs %>&amp;year=<%= hyear %>">
  <svg class="icon"><use xlink:href="<%=spriteHref%>#bi-printer-fill"></use></svg>
    Print <%= hyear %></a>
<a class="btn btn-outline-primary btn-sm mb-2 me-1" href="#email-modal" data-bs-toggle="modal" data-bs-target="#email-modal">
  <svg class="icon"><use xlink:href="<%=spriteHref%>#bi-envelope-fill"></use></svg>
  Weekly email</a>
<a class="btn btn-outline-primary btn-sm mb-2 me-1" href="/hebcal?v=1&amp;geo=<%= q.geo %>&amp;<%= geoUrlArgs %>&amp;year=now&amp;c=on&amp;s=on&amp;maj=on&amp;min=on&amp;mod=on&amp;mf=on&amp;ss=on&amp;nx=on">
  <svg class="icon"><use xlink:href="<%=spriteHref%>#bi-calendar3"></use></svg>
  <%= new Date().getFullYear() %> calendar</a>
<a class="btn btn-outline-primary btn-sm mb-2 me-1" title="RSS feed of candle lighting times" href="<%= rssUrl %>">
  <svg class="icon" style="width:1rem;height:1rem"><use xlink:href="<%=spriteHref%>#bi-rss-fill"></use></svg>
  RSS feed</a>
<a class="btn btn-outline-primary btn-sm mb-2 me-1" rel="nofollow" title="Candle lighting and Torah portion for your synagogue site" href="/link/?<%= geoUrlArgs %>">
  <svg class="icon"><use xlink:href="<%=spriteHref%>#bi-code-slash"></use></svg>
  Developer API</a>
</div>
<div class="d-print-none">
<hr class="mt-3">
<form action="/shabbat" method="get" autocomplete="off" id="shabbat-form">
<input type="hidden" name="geo" value="<%= q.geo %>" id="geo">
<input type="hidden" name="zip" value="<%= q.zip %>" id="zip">
<input type="hidden" name="city" value="<%= q.city %>" id="city">
<input type="hidden" name="geonameid" value="<%= q.geonameid %>" id="geonameid">
<%- await include('partials/city-and-havdalah.ejs') -%>
  <button type="submit" class="btn btn-primary">
    <svg class="icon mb-1"><use xlink:href="<%=spriteHref%>#gi-candle"></use></svg>
    Get Shabbat Times</button>
</form>
<hr>
<p><%= Shabbat %> times for world cities</p>
<ul class="bullet-list-inline">
<li><a href="/shabbat?geonameid=4347778&amp;b=18&amp;M=on&amp;lg=s">Baltimore</a></li>
<li><a href="/shabbat?geonameid=4930956&amp;b=18&amp;M=on&amp;lg=s">Boston</a></li>
<li><a href="/shabbat?geonameid=4148411&amp;b=18&amp;M=on&amp;lg=s">Boca Raton</a></li>
<li><a href="/shabbat?geonameid=5110302&amp;b=18&amp;M=on&amp;lg=s">Brooklyn</a></li>
<li><a href="/shabbat?geonameid=4887398&amp;b=18&amp;M=on&amp;lg=s">Chicago</a></li>
<li><a href="/shabbat?geonameid=281184&amp;b=40&amp;M=on&amp;lg=s">Jerusalem</a></li>
<li><a href="/shabbat?geonameid=5100280&amp;b=18&amp;M=on&amp;lg=s">Lakewood, NJ</a></li>
<li><a href="/shabbat?geonameid=2643743&amp;b=18&amp;M=on&amp;lg=s">London</a></li>
<li><a href="/shabbat?geonameid=5368361&amp;b=18&amp;M=on&amp;lg=s">Los Angeles</a></li>
<li><a href="/shabbat?geonameid=2158177&amp;b=18&amp;M=on&amp;lg=s">Melbourne</a></li>
<li><a href="/shabbat?geonameid=4164138&amp;b=18&amp;M=on&amp;lg=s">Miami</a></li>
<li><a href="/shabbat?geonameid=6077243&amp;b=18&amp;M=on&amp;lg=s">Montreal</a></li>
<li><a href="/shabbat?geonameid=5128581&amp;b=18&amp;M=on&amp;lg=s">New York</a></li>
<li><a href="/shabbat?geonameid=5391959&amp;b=18&amp;M=on&amp;lg=s">San Francisco</a></li>
<li><a href="/shabbat?geonameid=5809844&amp;b=18&amp;M=on&amp;lg=s">Seattle</a></li>
<li><a href="/shabbat?geonameid=293397&amp;b=18&amp;M=on&amp;lg=s">Tel Aviv</a></li>
<li><a href="/shabbat?geonameid=6167865&amp;b=18&amp;M=on&amp;lg=s">Toronto</a></li>
<li><a href="/shabbat?geonameid=4140963&amp;b=18&amp;M=on&amp;lg=s">Washington, DC</a></li>
<li><a href="/shabbat/browse/">More ...</a></li>
</ul>
</div><!-- .d-print-none -->
</div><!-- .col-md-8 -->
<div class="col-md-4 d-print-none" role="complementary">
<h6 class="text-muted mb-1">Advertisement</h6>
<script async src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js?client=ca-pub-7687563417622459"
     crossorigin="anonymous"></script>
<!-- responsive textonly -->
<ins class="adsbygoogle"
     style="display:block"
     data-ad-client="ca-pub-7687563417622459"
     data-ad-slot="5981467974"
     data-ad-format="auto"
     data-full-width-responsive="true"></ins>
<script>
(adsbygoogle = window.adsbygoogle || []).push({});
</script>
</div><!-- .col-md-4 -->
</div><!-- .row -->

</div><!-- #content -->
<%- await include('partials/email-candles-modal.ejs') _%>
<%- await include('partials/footer.ejs', {footerScripts: {tooltip: true, typeahead: true}}) _%>
