{"@context":"https://schema.org","@type":"Event","name":"Parashat <%=parsha%>",
"startDate":"<%=saturday.format('YYYY-MM-DD')%>","endDate":"<%=saturday.format('YYYY-MM-DD')%>",
"url":"<%=parshaHref%>",
"eventAttendanceMode": "https://schema.org/OnlineEventAttendanceMode",
"location":{"@type": "VirtualLocation",
"url":"<%=parshaHref%>"}}</script>

