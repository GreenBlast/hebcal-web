<% items.forEach((item) => { -%>
<li <% if (locale == 'he') { %> lang="he" dir="rtl"<% } -%>>
<%= item.emoji %>
<% if (item.url) { -%>
<a href="<%= item.url %>"><%= item.desc -%></a>
<% } else { -%>
<%= item.desc -%>
<% } -%>
<%= item.emoji %>
<% if (location) { %> <span class="text-muted">in <%= location %></span><% } -%>
</li>
<% }); -%>
