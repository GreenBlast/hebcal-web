<% if (aliyot) { %>
<ol class="list-unstyled">
<% for (const aliyah of Object.values(aliyot)) {  %>
<li><%=aliyah.num%>: <a class="outbound" title="Hebrew-English text and commentary from Sefaria.org" href="<%=aliyah.href%>"><%=aliyah.verses%></a>
<%- await include('numverses.ejs', {v: aliyah.v}) -%>
<span class="text-muted d-print-none">&middot;</span>
<a class="outbound d-print-none" title="Tikkun.io - <%=aliyah.verses%>"
href="https://tikkun.io/#/r/<%=aliyah.bookId%>-<%=aliyah.b.split(':')[0]%>-<%=aliyah.b.split(':')[1]%>">
<svg class="icon icon-sm"><use xlink:href="<%=spriteHref%>#bi-book"></use></svg></a>
<% if (aliyah.reason) { %><small> // <%= aliyah.reason %></small><% } %>
<% } %>
</ol>
<% } %>
