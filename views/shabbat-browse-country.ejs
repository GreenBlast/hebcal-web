<%- await include('partials/header.ejs') -%>
<style>
ul.list-unstyled li {margin-bottom: 0.5em}
</style>
</head>
<body>
<%- await include('partials/navbar.ejs') -%>
<div class="container">
<div id="content">
<div class="row mt-3">
<div class="col">
<h2><%=countryName%><br><span class="h3 small text-muted">Shabbat Candle Lighting Times</span></h2>
<p class="lead"><time datetime="<%= friday.format('YYYY-MM-DD') %>"><%= friday.format('dddd, D MMMM YYYY') %></time> - Parashat <%= parsha %></a></p>
</div><!-- .col -->
</div><!-- .row -->
<div class="row">
<% for (const a1 of admin1) { -%>
<div id="<%=a1.id%>" class="col-md-4">
<h3><%=a1.name%></h3>
<ul class="list-unstyled">
<% for (const r of results.filter((r) => r.admin1 === a1.name)) { -%>
<li><a href="/shabbat?geonameid=<%= r.geonameid %>"><%= r.name %></a> <time datetime="<%= r.isoDate %>T<%= r.isoTime %>:00"><%= r.fmtTime %></time></li>
<% } -%>
</ul>
</div><!-- .col -->
<% } -%>
</div><!-- .row -->
</div><!-- #content -->
<%- await include('partials/footer.ejs') _%>
